<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >  
<mapper namespace="com.enn.commodity.synergistic.dao.OrgAuthDao" >  

  <resultMap type="com.enn.commodity.synergistic.entity.OrgAuth" id="orgauthResultMap">
		<id property="id" column="orgauth_id"/>
		<result property="version" column="orgauth_version"/>
		<result property="name" column="orgauth_name"/>
		<result property="comments" column="orgauth_comments"/>
		<result property="organizationId" column="orgauth_organizationId"/>
		<result property="organizationName" column="orgauth_organizationName"/>
		<result property="userId" column="orgauth_userId"/>
		<result property="userName" column="orgauth_userName"/>
		<result property="privateId" column="orgauth_privateId"/>
		<result property="dataType" column="orgauth_dataType"/>
		<result property="goodsType" column="orgauth_goodsType"/>
		<result property="grantor" column="orgauth_grantor"/>
		<result property="grantee" column="orgauth_grantee"/>
		<result property="authType" column="orgauth_authType"/>
		<result property="content" column="orgauth_content"/>
		<result property="authDate" column="orgauth_authDate"/>
		<result property="authStartTime" column="orgauth_authStartTime"/>
		<result property="authEndTime" column="orgauth_authEndTime"/>
		<result property="key" column="orgauth_key"/>
		
		
	</resultMap>



        

 <select id="SelectOrgAuthByObject" resultMap="orgauthResultMap" parameterType="com.enn.commodity.synergistic.entity.OrgAuth">
     SELECT *
     FROM orgauth o
    
     WHERE 1=1
     <if test="id!=null and id!=''" >
     and o.orgauth_id=#{id}
    </if>
    <if test="name!=null and name!=''" >
     and o.orgauth_name=#{name}
    </if>
    <if test="version!=null and version!=''" >
     and o.orgauth_version=#{version}
    </if>
    <if test="comments!=null and comments!=''" >
     and o.orgauth_comments=#{comments}
    </if>
    <if test="organizationId!=null and organizationId!=''" >
     and o.orgauth_organizationId=#{organizationId}
    </if>
    <if test="organizationName!=null and organizationName!=''" >
     and o.orgauth_organizationName=#{organizationName}
    </if>
    <if test="userId!=null and userId!=''" >
     and o.orgauth_userId=#{userId}
    </if>
    <if test="userName!=null and userName!=''" >
     and o.orgauth_userName=#{userName}
    </if>
    <if test="privateId!=null and privateId!=''" >
     and o.orgauth_privateId=#{privateId}
    </if>
    <if test="dataType!=null and dataType!=''" >
     and o.orgauth_dataType=#{dataType}
    </if>
    <if test="goodsType!=null and goodsType!=''" >
     and o.orgauth_goodsType=#{goodsType}
    </if>
    <if test="grantor!=null and grantor!=''" >
     and o.orgauth_grantor=#{grantor}
    </if>
    <if test="grantee!=null and grantee!=''" >
     and o.orgauth_grantee=#{grantee}
    </if>
    <if test="authType!=null and authType!=''" >
     and o.orgauth_authType=#{authType}
    </if>
    <if test="content!=null and content!=''" >
     and o.orgauth_content=#{content}
    </if>
    <if test="authDate!=null and authDate!=''" >
     and o.orgauth_authDate=#{authDate}
    </if>
    <if test="authStartTime!=null and authStartTime!=''" >
     and o.orgauth_authStartTime=#{authStartTime}
    </if>
    <if test="authEndTime!=null and authEndTime!=''" >
     and o.orgauth_authEndTime=#{authEndTime}
    </if>
    <if test="key!=null and key!=''" >
     and o.orgauth_key=#{key}
    </if>
    

  </select>
 
 
 
