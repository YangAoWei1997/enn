  <insert id="CreatePO"  keyColumn="po_id" parameterType="com.enn.commodity.synergistic.entity.PO">
    insert into po 
    (po_id,po_version,po_name,po_comments,po_organizationId,po_organizationName,
    po_userId,po_userName,po_privateId,po_dataType,po_goodsType,po_dataSourceId,po_dataSourceName,po_buyerId,po_buyerName,po_sellerId,po_sellerName,po_type,po_arrangementId,po_customerArrangementId,po_relatedPONumber,po_issuedDate,po_status,po_productId,po_productName,po_productType,po_carryPONumber,po_carrierId,po_carrierName,po_discontRate,po_payType,po_poType,po_plateno,po_payTypeName,po_salesPONumber,po_purchasePONumber,po_materialDetailId,po_completeWeight,po_carNumber) 
    values
    (#{id},#{version},#{name},#{comments},#{organizationId},#{organizationName},#{userId},
    #{userName},#{privateId},#{dataType},#{goodsType},#{dataSourceId},#{dataSourceName},#{buyerId},#{buyerName},#{sellerId},#{sellerName},#{type},
    #{arrangementId},#{customerArrangementId},#{relatedPONumber},#{issuedDate},#{status},#{productId},#{productName},#{productType},#{carryPONumber},#{carrierId},#{carrierName},#{discontRate},#{payType},#{poType},#{plateno},#{payTypeName},#{salesPONumber},#{purchasePONumber},#{materialdetail.id},#{completeWeight},#{carNumber})
  </insert>
  
