  <update id="UpdatePO" parameterType="com.enn.commodity.synergistic.entity.PO">
     UPDATE po
     <set>
       <if test="status!=null">po_status=#{status},</if>
       <if test="completeWeight!=null">po_completeWeight=#{completeWeight},</if>
       <if test="completeWeight!=null">po_carNumber=#{carNumber},</if>
     </set>
     WHERE po_id=#{id}
  </update>
  
  <update id="UpdateMaterialDetail" parameterType="com.enn.commodity.synergistic.entity.MaterialDetail">
     UPDATE materialdetail
     <set>
       <if test="quantity!=null">materialdetail_quantity=#{quantity},</if>
       <if test="quantityUnit!=null">materialdetail_quantityUnit=#{quantityUnit},</if>
       <if test="priceNoTax!=null">materialdetail_priceNoTax=#{priceNoTax},</if>
       <if test="priceWithTax!=null">materialdetail_priceWithTax=#{priceWithTax},</if>
       <if test="totalPriceNoTax!=null">materialdetail_totalPriceNoTax=#{totalPriceNoTax},</if>
       <if test="totalPriceWithTax!=null">materialdetail_totalPriceWithTax=#{totalPriceWithTax},</if>
       <if test="priceQualityUnit!=null">materialdetail_priceQualityUnit=#{priceQualityUnit},</if>
       <if test="realTotalPrice!=null">materialdetail_realTotalPrice=#{realTotalPrice},</if>
       <if test="realPrice!=null">materialdetail_realPrice=#{realPrice},</if>
     </set>
     WHERE materialdetail_id=#{id}
  </update>
  
  
  
  <delete id="DeletePO" parameterType="com.enn.commodity.synergistic.entity.PO">
     DELETE FROM
     po
     WHERE
     po_id=#{id}
  </delete>
  
  <delete id="DeleteMaterialDetail" parameterType="com.enn.commodity.synergistic.entity.PO">
     DELETE FROM
     materialdetail
     WHERE
     materialdetail_id=#{materialdetail.id}
  </delete>
  
  <delete id="DeletePayment" parameterType="com.enn.commodity.synergistic.entity.PO">
     DELETE FROM
     payment
     WHERE
     payment_poId=#{id}
  </delete>
  
  <delete id="DeletePaymentDetail" parameterType="com.enn.commodity.synergistic.entity.Payment">
     DELETE FROM
     paymentdetail
     WHERE
     paymentdetail_id=#{paymentdetail.id}
  </delete>
