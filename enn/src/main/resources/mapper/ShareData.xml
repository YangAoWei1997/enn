<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >  
<mapper namespace="com.enn.commodity.synergistic.dao.ShareDataDao" >  
 <select id="SelectShareDataByObject" resultType="com.enn.commodity.synergistic.entity.ShareData">
     SELECT ID,name,comments,dataType,goodsType,resourceOrgID,resourceOrgName,resourceOrgType,resourceSystemID,resourceSystemName,destinations,productID,productName,requestType,price,quantity,quantityUnit,detail,bizID,expired,status,issuedDate
     FROM sharedata
     WHERE 1=1
     <if test="name!=null and name!=''" >
     and name  LIKE CONCAT(CONCAT('%', #{name}), '%')
     </if>
     <if test="ID!=null and ID!=''" >
     and ID=#{ID}
     </if>
    <if test="comments!=null and comments!=''" >
     and comments=#{comments}
     </if>
    <if test="dataType!=null and dataType!=''" >
     and dataType=#{dataType}
     </if>
     <if test="goodsType!=null and goodsType!=''" >
     and goodsType=#{goodsType}
     </if>
     <if test="resourceOrgID!=null and resourceOrgID!=''" >
     and resourceOrgID=#{resourceOrgID}
     </if>
     <if test="resourceOrgName!=null and resourceOrgName!=''" >
     and resourceOrgName=#{resourceOrgName}
     </if>
     <if test="resourceOrgType!=null and resourceOrgType!=''" >
     and resourceOrgType=#{resourceOrgType}
     </if>
     <if test="resourceSystemID!=null and resourceSystemID!=''" >
     and resourceSystemID=#{resourceSystemID}
     </if>
     <if test="resourceSystemName!=null and resourceSystemName!=''" >
     and resourceSystemName=#{resourceSystemName}
     </if>
     <if test="destinations!=null and destinations!=''" >
     and destinations=#{destinations}
     </if>
     <if test="productID!=null and productID!=''" >
     and productID=#{productID}
     </if>
     <if test="productName!=null and productName!=''" >
     and productName=#{productName}
     </if>
     <if test="requestType!=null and requestType!=''" >
     and requestType=#{requestType}
     </if>
     <if test="price!=null and price!=''" >
     and price=#{price}
     </if>
     <if test="quantity!=null and quantity!=''" >
     and quantity=#{quantity}
     </if>
     <if test="quantityUnit!=null and quantityUnit!=''" >
     and quantityUnit=#{quantityUnit}
     </if>
     <if test="detail!=null and detail!=''" >
     and detail=#{detail}
     </if>
     <if test="bizID!=null and bizID!=''" >
     and bizID=#{bizID}
     </if>
     <if test="expired!=null and expired!=''" >
     and expired=#{expired}
     </if>
     <if test="status!=null and status!=''" >
     and status=#{status}
     </if>
     <if test="issuedDate!=null and issuedDate!=''" >
     and issuedDate=#{issuedDate}
     </if>
  </select>
 
 <insert id="CreateShareData"  keyColumn="ID" parameterType="com.enn.commodity.synergistic.entity.ShareData">
    insert into sharedata 
    (ID,name,comments,dataType,goodsType,resourceOrgID,resourceOrgName,resourceOrgType,resourceSystemID,resourceSystemName,destinations,productID,productName,requestType,price,quantity,quantityUnit,detail,bizID,expired,status,issuedDate) 
    values
    (#{ID},#{name},#{comments},#{dataType},#{goodsType},#{resourceOrgID},#{resourceOrgName},#{resourceOrgType},#{resourceSystemID},#{resourceSystemName},#{destinations},#{productID},#{productName},#{requestType},#{price},#{quantity},#{quantityUnit},#{detail},#{bizID},#{expired},#{status},#{issuedDate})
  </insert>
 
 <update id="UpdateShareData" parameterType="com.enn.commodity.synergistic.entity.ShareData">
     UPDATE sharedata
     <set>
       <if test="name!=null">name=#{name},</if>
       <if test="comments!=null">comments=#{comments},</if>
       <if test="dataType!=null">dataType=#{dataType},</if>
       <if test="goodsType!=null">goodsType=#{goodsType},</if>
       <if test="resourceOrgID!=null">resourceOrgID=#{resourceOrgID},</if>
       <if test="resourceOrgName!=null">resourceOrgName=#{resourceOrgName},</if>
       <if test="resourceOrgType!=null">resourceOrgType=#{resourceOrgType},</if>
       <if test="resourceSystemID!=null">resourceSystemID=#{resourceSystemID},</if>
       <if test="resourceSystemName!=null">resourceSystemName=#{resourceSystemName},</if>
       <if test="destinations!=null">destinations=#{destinations},</if>
       <if test="productID!=null">productID=#{productID},</if>
       <if test="productName!=null">productName=#{productName},</if>
       <if test="requestType!=null">requestType=#{requestType},</if>
       <if test="price!=null">price=#{price},</if>
       <if test="quantity!=null">quantity=#{quantity},</if>
       <if test="quantityUnit!=null">quantityUnit=#{quantityUnit},</if>
       <if test="detail!=null">detail=#{detail},</if>
        <if test="bizID!=null">bizID=#{bizID},</if>
       <if test="expired!=null">expired=#{expired},</if>
       <if test="status!=null">status=#{status},</if>
       <if test="issuedDate!=null">issuedDate=#{issuedDate},</if>
     </set>
     WHERE ID=#{ID}
  </update>
 
 <delete id="DeleteShareDataOrg" parameterType="com.enn.commodity.synergistic.entity.ShareData">
     DELETE FROM
     sharedata
     WHERE
     ID=#{ID}
  </delete>
 
